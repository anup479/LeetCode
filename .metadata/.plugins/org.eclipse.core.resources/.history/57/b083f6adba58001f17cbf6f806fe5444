package com.nt.core;

import java.util.Scanner;

//Write a Java program to find continuous sub array whose sum is equal to a given number?
// create a matrix in java ... 
public class Bud {

	public static void main(String[] args) {

		// create Scanner

		Scanner sc = new Scanner(System.in);

		System.out.println("eneter the values of row ");
		int row = sc.nextInt();

		System.out.println("enter the values of Coloums");
		int col = sc.nextInt();

		// defining two D arrays to hold matrix Data

		int[][] matrix = new int[row][col];

		for (int i = 0; i < row; i++) {

			for (int j = 0; j < col; j++) {

				matrix[i][j] = sc.nextInt();
			}
		}
		System.out.println(" your matrix is ");

		for (int i = 0; i < row; i++) {
			for (int j = 0; j < col; j++) {

				System.out.println(matrix[i][j] + " this is your matix");
			}
		}

	}

}
