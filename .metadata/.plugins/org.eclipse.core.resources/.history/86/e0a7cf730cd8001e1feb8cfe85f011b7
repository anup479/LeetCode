package com.javaconceptOfday;

import java.util.HashMap;
import java.util.Map;

public class DuplicatesINString {

	public static void dupli(String name) {
		// butter
		Map<Character, Integer> hMap = new HashMap<>();
		char[] charArray = name.toCharArray();
		for (char cc : charArray) {

			// if its present in the hasHmap
			hMap.put(cc, hMap.get(cc) + 1);
		}
		else 
		{
			// if char is not present in the hmap,putting this char.value with 1 as its value.
			hMap.put(, 1);
		}

	}

	public static void main(String[] args) {

	}
}
