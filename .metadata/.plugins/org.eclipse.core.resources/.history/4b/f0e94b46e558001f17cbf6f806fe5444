package com.nt.coreJava;

public class A {
	public static void main(String[] args) {
		String name = "Hello , this is a simple Match problem";

		for (int i = 0; i < name.length(); i++) {

			if (name.charAt(i) == 'a' || name.charAt(i) == 'e' || name.charAt(i) == 'i' || name.charAt(i) == 'o'
					|| name.charAt(i) == 'u')
				;

		}
	}
}
